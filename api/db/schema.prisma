datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

// A blogPost model with id, title, body and createdAt fields.

model Post {
  id      Int @id @default(autoincrement())
  title   String
  body    String
  createdAt DateTime @default(now())
}